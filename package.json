{
  "name": "wp-pulse",
  "version": "1.0.0",
  "description": "Pulse tracks user activity for logged in users in stunning detail.",
  "main": "pulse.php",
  "scripts": {
    "assets:build": "wp-scripts build assets/src/js/admin-dashboard.js --output-path=assets/build",
    "assets:start:hot": "wp-scripts start assets/src/js/admin-dashboard.js --output-path=assets/build --hot",
    "assets:start": "wp-scripts start assets/src/js/admin-dashboard.js --output-path=assets/build",
    "composer": "wp-env run cli --env-cwd=wp-content/plugins/pulse composer",
    "env:cli": "wp-env run cli wp",
    "env:destroy": "wp-env destroy",
    "env:logs": "wp-env logs",
    "env:reset": "wp-env destroy && wp-env start",
    "env:shell": "wp-env run cli bash",
    "env:start": "wp-env start",
    "env:stop": "wp-env stop",
    "format-php": "npm run composer fix",
    "format-js": "wp-scripts lint-js assets/src/js/ --fix",
    "lang:i18n": "wp-env run cli wp i18n make-pot wp-content/plugins/pulse/ wp-content/plugins/pulse/languages/pulse-en_US.po --exclude=assets/build,node_modules,vendor",
    "lang:mo": "wp-env run cli wp i18n make-mo wp-content/plugins/pulse/languages/pulse-en_US.po wp-content/plugins/pulse/languages/pulse-en_US.mo",
    "lang": "npm run lang:i18n && npm run lang:mo",
    "lint-php": "npm run composer lint",
    "lint-js": "wp-scripts lint-js assets/src/js/"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "pulse",
    "activity",
    "log",
    "track"
  ],
  "author": "Scott Weaver",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.83.1",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@types/wordpress__api-fetch": "^3.2.4",
    "@types/wordpress__url": "^2.3.0",
    "@wordpress/env": "^10.29.0",
    "@wordpress/scripts": "^30.22.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tdlm/pulse.git"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "@uidotdev/usehooks": "^2.4.1",
    "nuqs": "^2.5.1",
    "react-timeago": "^8.3.0"
  }
}
